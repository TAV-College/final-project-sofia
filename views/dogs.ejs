<%- include("parts/header") %>

<div class="container my-5">
  <h1 class="display-4 fw-bold text-center my-4 title-blue">Dogs for Adoption</h1>

  <table class="table table-striped align-middle custom-table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Age</th>
        <th>Breed</th>
        <th>State</th>
        <th>Species</th>
      </tr>
    </thead>
    <tbody>
      <% dogs.forEach(function(dog) { %>
        <tr>
          <td><%= dog.name %></td>
          <td><%= dog.age %></td>
          <td><%= dog.breed %></td>
          <td><%= dog.state %></td>
          <td><%= dog.species %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</div>

<%- include("parts/footer") %>
